{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "angular",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "angular/di": "off"
    }
}